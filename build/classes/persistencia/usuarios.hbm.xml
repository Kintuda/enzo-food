<?xml version="1.0" encoding="UTF-8"?>
http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="enzo-food" table="usuarios">
        <id column="id" name="id" type="java.lang.Long">
            <generator class="native"></generator>
        </id>
        <property column="nome" name="nome" length="200" type="java.lang.String"/>
        <property column="cidade" name="cidade" length="120" type="java.lang.String"/>
        <property column="endereco" name="endereco" type="java.lang.String"/>
    </class>
</hibernate-mapping>